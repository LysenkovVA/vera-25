{
  "name": "vera-25",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate:diff": "npx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/migrations/0_init/migration.sql",
    "prisma:migrate:resolve": "npx prisma migrate resolve --applied 0_init",
    "prisma:migrate:dev": "npm exec prisma migrate dev",
    "prisma:client:generate": "npm exec prisma generate",
    "prisma:db:push": "npm exec prisma db push",
    "prisma:db:seed": "npx prisma db seed",
    "prisma:initdb": "npm run prisma:migrate --name create_db && npm run prisma:db:seed",
    "prisma:db:dev": "npm run prisma:db:push && npm run prisma:db:seed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@ant-design/nextjs-registry": "^1.0.0",
    "@auth/prisma-adapter": "^2.4.2",
    "@prisma/client": "^5.18.0",
    "@reduxjs/toolkit": "^2.2.7",
    "antd": "^5.19.3",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.12",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.19",
    "pg": "^8.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@eslint/js": "^9.9.0",
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/addon-onboarding": "^8.2.6",
    "@storybook/blocks": "^8.2.6",
    "@storybook/nextjs": "^8.2.6",
    "@storybook/react": "^8.2.6",
    "@storybook/test": "^8.2.6",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-storybook": "^0.8.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^15.9.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prisma": "^5.18.0",
    "sass": "^1.77.8",
    "storybook": "^8.2.6",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.2.0"
  }
}
